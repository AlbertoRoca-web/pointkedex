{
  "scriptFile": "__init__.py",
  "bindings": [
    {
      "authLevel": "anonymous",
      "type": "httpTrigger",
      "direction": "in",
      "name": "req",
      "methods": [ "post" ],
      "route": "predict"        // becomes  https://<app>.azurewebsites.net/predict
    },
    {
      "type": "http",
      "direction": "out",
      "name": "res"
    }
  ]
}
